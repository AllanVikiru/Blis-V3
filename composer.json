{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=7.1.3",
        "emadadly/laravel-uuid": "1.*",
        "fideloper/proxy": "~4.0",
        "genealabs/laravel-caffeine": "^0.7.1",
        "ilabafrica/emrinterface": "v0.2.0",
        "ilabafrica/equipmentinterface": "v0.1.2",
        "laravel/dusk": "^4.0",
        "laravel/framework": "5.6.*",
        "laravel/passport": "^5.0",
        "laravel/tinker": "~1.0",
        "tecnickcom/tcpdf": "^6.2",
        "tymon/jwt-auth": "^1.0.0-rc.2",
        "webpatser/laravel-uuid": "^3.0",
        "zizaco/entrust": "5.2.x-dev"
    },
    "require-dev": {
        "filp/whoops": "~2.0",
        "fzaninotto/faker": "~1.4",
        "mockery/mockery": "~1.0",
        "phpunit/phpunit": "~7.0",
        "symfony/thanks": "^1.0"
    },
    "autoload": {
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "psr-4": {
            "App\\": "app/",
                "Tracker\\SpecimenTracker\\": "packages/tracker/specimentracker/src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
   "extra": { "laravel": { "dont-discover": [ "ilabafrica/specimentracker" ] } },
   
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
